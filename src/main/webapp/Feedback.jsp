<script type="text/javascript">
    var registerForm = $('#webenquiryform');

    registerForm.submit(function (e) {



        e.preventDefault();
        var formData = new FormData(document.getElementById('webenquiryform'));
        $.ajax({
            type: registerForm.attr('method'),
            url: registerForm.attr('action'),
            // data: registerForm.serialize(),
            data: formData,
            processData: false,
            contentType: false,
            enctype: 'multipart/form-data',
            success: function (data) {
                console.log('Submission was successful.');
                var jsonObject = JSON.parse(data);
                if (jsonObject.code === true) {

                    alert("Thank you for contact Us. We will reach out shortly");
                    location.reload();
                } else {
                    alert(jsonObject.message);
                }
            },
            error: function (data) {
                console.log('An error occurred.');
                console.log(data);
            },
        });
    });
</script>

